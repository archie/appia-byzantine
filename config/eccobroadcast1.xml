<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE appia PUBLIC "-//DTDName//DTD//EN"
	"xml/appiaxml.dtd">
<appia>
	<template name="ecco_t">
		<session name="tcp_s" sharing="global">
			<protocol>net.sf.appia.protocols.tcpcomplete.TcpCompleteLayer</protocol>
		</session>
		<session name="sign_s" sharing="private">
			<protocol>net.sf.appia.protocols.signing.SignatureLayer</protocol>
		</session>
		<session name="ecco_s" sharing="private">
			<protocol>net.sf.appia.protocols.echobroadcast.EchoBroadcastLayer</protocol>
		</session>
    	<session name="app_layer" sharing="private">
			<protocol>eu.emdc.testing.ApplicationLayer</protocol>
		</session>

	</template>
	<channel name="ecco_c" template="ecco_t" initialized="yes">
		<chsession name="ecco_s">
      		<parameter name="myrank">0</parameter>
			<parameter name="processes">config/processes.conf</parameter>
		</chsession>
		
		<chsession name="sign_s">
			<parameter name="user_alias">user0</parameter>
        	<parameter name="store_type">JKS</parameter>
        	<parameter name="keystore_file">config/user0.jks</parameter>
        	<parameter name="keystore_pass">123456</parameter>
			<parameter name="trustedcerts_file">config/usercerts.jks</parameter>
			<parameter name="trustedcerts_pass">123456</parameter>
		</chsession>
	</channel>
</appia>